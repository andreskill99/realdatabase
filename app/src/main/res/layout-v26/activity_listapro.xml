<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginHorizontal="10dp"
    android:background="#448AFF"
    android:scrollbars="vertical"
    app:barrierMargin="?attr/listPreferredItemHeightLarge"
    app:cardCornerRadius="20dp"
    tools:context=".listapro">


    <ListView
        android:id="@+id/listview1"
        style="@android:style/Widget.Material.Light.ListPopupWindow"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="22dp"
        android:layout_marginEnd="5dp"
        android:layout_marginBottom="22dp"
        android:background="#40C4FF"
        android:backgroundTint="#FFFFFF"

        android:backgroundTintMode="src_over"
        android:fastScrollAlwaysVisible="true"
        android:fastScrollEnabled="true"
        android:filterTouchesWhenObscured="true"
        android:focusable="auto"
        android:focusableInTouchMode="true"
        android:footerDividersEnabled="true"
        android:foreground="@mipmap/doraemon"
        android:foregroundTint="#11000000"
        android:paddingBottom="@dimen/fab_margin"
        android:scrollbarAlwaysDrawHorizontalTrack="true"
        android:scrollbarAlwaysDrawVerticalTrack="true"
        android:scrollbarStyle="outsideInset"
        android:scrollingCache="true"
        android:smoothScrollbar="true"
        android:soundEffectsEnabled="true"
        android:splitMotionEvents="true"
        android:stackFromBottom="false"
        android:stateListAnimator="@anim/nav_default_pop_enter_anim"
        android:theme="@style/SplashTheme"
        android:touchscreenBlocksFocus="true"
        android:transformPivotX="@dimen/fab_margin"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>
